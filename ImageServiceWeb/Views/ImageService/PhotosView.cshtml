@model ImageServiceWeb.Models.PhotoList

@{
    ViewBag.Title = "PhotosView";
}

<h2>PhotosView</h2>

<link href="~/Css/photos-view.css" rel="stylesheet" type="text/css" />

<img src="~/Photos/download.jpg">
<body class="vsc-initialized">
    <div class="container" id="tourpackages-carousel">

        <div class="row">
            @foreach (var item in Model.GetPhotos())
            {
                <div class="col-xs-18 col-sm-6 col-md-3">
                    <div class="img-rounded">
                        <img src="C:/Users/Larry/source/repos/ImageService/ImageServiceWeb/Photos/to/Thumbnails/2018/5/download.jpg">
                        <div class="caption">
                            <h4>Thumbnail label</h4>
                            <p>Creation date:</p>
                            <p>Year: @Html.DisplayFor(modelItem => item.Year), Month: @Html.DisplayFor(modelItem => item.Month)</p>
                            <p><a href="#" class="btn btn-info btn-xs" role="button">View</a> <a href="#" class="btn btn-default btn-xs" role="button">Delete</a></p>
                        </div>
                    </div>
                </div>
            }
        </div><!-- End row -->
    </div><!-- End container -->
    <script type="text/javascript">
    </script>
</body>