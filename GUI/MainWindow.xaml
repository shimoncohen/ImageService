<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="ImageService" Height="450" Width="800">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792">
            <TabItem Header="Settings">
                <local:SettingsView DataContext="{Binding SettingsViewModel}"
                                    VerticalAlignment="Top"
                                    Height="391">
                </local:SettingsView>
            </TabItem>
            <TabItem Header="Logs">
                <local:LogsView DataContext="{Binding LogsViewModel}"
                                VerticalAlignment="Bottom"
                                Height="391" Loaded="SettingsView_Loaded">
                </local:LogsView>
            </TabItem>
            <TextBlock Height="112" TextWrapping="Wrap" Text="TextBlock" Width="212"/>
            <StackPanel Height="100" Width="100"/>
        </TabControl>
    </Grid>
</Window>
